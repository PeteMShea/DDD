            <kind>1</kind>
            <string>playery = obj_player.y;
playerx = obj_player.x;

if messageone == false
{
    if playery &lt; 500
        {
            messageone = true;
            alphalevel = 0;          
        }
}

if messageone == true &amp;&amp; messagetwoappear == false
{
    if playery &lt; 400
        {
            messagetwoappear = true;
            oldlandspeed = obj_player.minlandspeed;
            obj_player.minlandspeed = 1.0;      //temporarily make it easier to land         
        }
} 

if messagetwoappear == true &amp;&amp; messagetwoseen == false
{
    if obj_player.landing == 3
        {
            messagetwoseen = true;                      
        }
}

if playery &lt; 150 || playerx &lt; 1300
{
    messageone = true;
    messagetwoseen = true;
    messagthreeseen = true;
    messagefourseen = true;
    //extralandingmessageseen = true;
}

if messagetwoseen == true &amp;&amp; messagethreeappear == false &amp;&amp; messagefourseen == false
{
    if instance_exists(obj_StatusPanel)
    {
        if obj_StatusPanel.buttonsappeared == true
            {                
                if global.playerHealth &lt; global.startHealth extralandingmessage = true;
                if extralandingmessage == false messagethreeappear = true;
                obj_WeaponryButton.locked = true;
                obj_DatabaseButton.locked = true;
                obj_EquipButton.locked = true;
                obj_player.minlandspeed = oldlandspeed;            
            } 
    }
}

if extralandingmessage == true &amp; extralandingmessageappear == false extralandingmessageappear = true;

if extralandingmessageappear == true &amp; extralandingmessageseen == false
{
    extralandingmessagetimeout -=1;
    if extralandingmessagetimeout &lt;= 0
        {
            extralandingmessageseen = true;
            if messagethreeappear == false messagethreeappear = true;        
        }
}




if messagethreeappear == true &amp; messagethreeseen == false
{
    messagethreetimeout -=1;
    if messagethreetimeout &lt;= 0
        {
            messagethreeseen = true;          
        }
}

if messagethreeseen == true &amp; messagefourappear == false
{
    messagefourappear = true;
}

if messagethreeappear == true &amp;&amp; messagefourseen == false
{
    if obj_StatusPanel.panelsdismissed == true
    {
        messagethreeseen = true;
        messagefourseen = true;    
    }
}

if messagefourseen == true &amp;&amp; messagefiveappear == false
{
    if playery &lt; 500
        {
            messagefiveappear = true;       
        }
}

if messagefiveappear == true &amp; messagefiveseen == false
{
    messagefivetimeout -=1;
    if messagefivetimeout &lt;= 1
        {
            messagefiveseen = true;
        }
}

if messagefiveseen == true &amp;&amp; messagesixappear == false
{
    if playerx &lt; 1100
    {
        messagesixappear = true;
    }
}

if messagesixappear == true &amp; messagesixseen == false
{
    if instance_exists(obj_bullet)
        {
            messagesixshoottimeout -=1;
            //show_debug_message(string(messagesixshoottimeout));
        }
    if messagesixshoottimeout &lt;= 0
        {
            messagesixseen = true;      
        }

}

if messagesixappear == true &amp;&amp; playery &gt; 650
{
    messagesixseen = true;
}

if messagesixseen == true &amp;&amp; messagesevenappear == false
{
    if playery &gt; 750 || global.playerGold &gt; 0
        {
            messagesevenappear = true;
        }
}


if messagesixseen == true &amp;&amp; playery &gt; 750
{
    messagesevenseen = true;
}











